CREATE TABLE `myapp`.`zones` (
  `id_zone` INT UNSIGNED NOT NULL AUTO_INCREMENT,
  `zone_state` VARCHAR(60) NULL,
  `zone_country` VARCHAR(2) NULL,
  `zone_law` VARCHAR(255) NULL,
  `zone_law_description` TEXT NULL,
  `zone_requirements` VARCHAR(45) NULL,
  `zone_penalties` VARCHAR(120) NULL,
  `zone_effective` DATE NULL,
  `zone_meta` TEXT NULL,
  PRIMARY KEY (`id_zone`));
