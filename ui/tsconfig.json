{
	"compilerOptions": {
		"target": "ES2020",
		"lib": ["DOM", "ESNext"],
		"module": "ESNext",
		"moduleResolution": "Node",
		"jsx": "react-jsx",
		"allowJs": false,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true
	},
	"include": ["src"],
	"exclude": ["node_modules", "dist"]
}
